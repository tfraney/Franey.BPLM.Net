{
  "AppConfig": {
    "Environment": "DEV"
  },
  "Logging": {
    "LogLevel": { // No provider, LogLevel applies to all the enabled providers.
      "Default": "Information",
      "Microsoft": "Trace",
      "Microsoft.Hosting.Lifetime": "Trace"
    },
    "Debug": { // Debug provider.
      "LogLevel": {
        "Default": "Trace" // Overrides preceding LogLevel:Default setting.
      }
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "EventSource": {
      "LogLevel": {
        "Microsoft": "Trace"
      }
    },
    "EventLog": {
      "LogLevel": {
        "Microsoft": "Trace"
      }
    }
  }
}
